<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
  <Id>f02f27c3-5023-4069-a382-b9eb9c9dd9a3</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Moxtra Inc.</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Moxtra-Tony" />
  <Description DefaultValue="Moxtra. Teamwork Reimagined. Moxtra is the collaboration companion that works the way teams do: flexibly and simply, in real-time or anytime." />
  <IconUrl DefaultValue="https://www.moxtra1.com/tony/outlook/Images/MoxtraLogo.PNG" />
  <HighResolutionIconUrl DefaultValue="https://www.moxtra1.com/tony/outlook/Images/MoxtraLogo128x128.PNG" />
  <SupportUrl DefaultValue="http://support.moxtra.com/" />
  <AppDomains>
    <AppDomain>https://www.moxtra.com</AppDomain>
    <AppDomain>https://sandbox.moxtra.com</AppDomain>
    <AppDomain>https://localhost</AppDomain>
    <AppDomain>https://www.moxtra1.com</AppDomain>
    <AppDomain>https://apisandbox.moxtra.com</AppDomain>
    <AppDomain>https://api.moxtra.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://www.moxtra1.com/tony/outlook/AppRead/Home/Home.html" />
        <RequestedHeight>450</RequestedHeight>
      </DesktopSettings>
      <TabletSettings>
        <SourceLocation DefaultValue="https://www.moxtra1.com/tony/outlook/AppRead/Home/Home.html" />
        <RequestedHeight>450</RequestedHeight>
      </TabletSettings>
    </Form>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://www.moxtra1.com/tony/outlook/AppCompose/Home/Home.html" />
      </DesktopSettings>
      <TabletSettings>
        <SourceLocation DefaultValue="https://www.moxtra1.com/tony/outlook/AppCompose/Home/Home.html" />
      </TabletSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit" />
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read" />
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
</OfficeApp>
